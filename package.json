{
  "name": "@shawndrake2/vue-components",
  "version": "0.1.4",
  "license": "MIT",
  "description": "Collection of VueJS components",
  "keywords": [
    "VueJS",
    "Vue",
    "Bulma",
    "components"
  ],
  "main": "./dist/vue-components.common.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shawndrake2/vue-components.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib --name vue-components ./module/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "buefy": "^0.8.3",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "files": [
    "dist/*",
    "module/*",
    "src/components"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
